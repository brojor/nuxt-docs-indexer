{
  "name": "nuxt-docs-indexer",
  "version": "1.0.0",
  "description": "CLI tool to index Nuxt docs.",
  "author": {
    "name": "BroJor",
    "url": "https://brojor.me"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brojor/nuxt-docs-indexer.git"
  },
  "keywords": [
    "nuxt",
    "docs",
    "indexer"
  ],
  "main": "dist/index.js",
  "bin": {
    "nuxt-docs-indexer": "dist/index.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsup",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "dependencies": {
    "@poppinss/string": "^1.3.0",
    "gray-matter": "^4.0.3",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
